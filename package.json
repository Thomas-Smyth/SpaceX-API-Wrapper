  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomas-Smyth/SpaceX-API-Wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/Thomas-Smyth/SpaceX-API-Wrapper/issues"
  },
  "homepage": "ttps://github.com/Thomas-Smyth/SpaceX-API-Wrapper#readme",
  "main": "index.js",
  "scripts": {
